<% if @feed_items.any? %>
    <ol class= "microposts">
<!-- Rails knows to call _microposts.html.erb since each elements of @feed_items is of the Micropost ckass -->
<!-- Rails will look for a partial with a corresponding name in the views directory of the given resource -->
        <%= render @feed_items %>
    </ol>
    <%= will_paginate @feed_items %>
<% end %>